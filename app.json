{
  "expo": {
    "name": "mofid-trip",
    "slug": "mofid-trip",
    "version": "1.0.0",
    "scheme": "com.mofidtrip",
    "orientation": "portrait",
    "icon": "./assets/image/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/image/splash-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/image/splash-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mofidtrip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "http://localhost:8081/",
              "pathPrefix": "/successPayment"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.mofidtrip"
    },
    "web": {
      "favicon": "./assets/image/adaptive-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e7a0d71a-f8b1-4da4-b820-549b93828ec1"
      }
    },
    "sdkVersion": "49.0.0",
    "entryPoint": ".vscode/exponentIndex.js"
  },
  "name": "mofid-trip"
}